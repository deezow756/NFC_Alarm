<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NFCAlarm.AlarmPage">
    <ContentPage.Content>
        <StackLayout>
            <Switch x:Name="IsWriteable"/>
            <Switch x:Name="IsConnected"/>
            <Switch x:Name="IsNDEFSupported"/>
            <Label Text="Records"/>
            <ListView x:Name="listRecords"/>
            <ListView x:Name="listTechs"/>
            <Button x:Name="btnWriteTag" Text="WriteTag" Clicked="HandleClicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>